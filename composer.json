{
  "name": "quansitech/qscmf-http-tracer",
  "description": "qscmf http client trace logger",
  "type": "library",
  "keywords": ["thinkphp", "qscmf", "http", "client","logger", "trace"],
  "license": "MIT",
  "authors": [
    {
      "name": "xhiny",
      "email": "axhiny@163.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "tiderjian/think-core":  ">=11.0.0",
    "monolog/monolog": "^2.0"
  },
  "require-dev": {
      "guzzlehttp/guzzle": "^6.4 || ^7.4" 
  },
  "suggest": {
      "guzzlehttp/guzzle": "To use the Guzzle integration, install Guzzle (^6.4 or ^7.4)."
  },
  "autoload": {
    "psr-4": {
      "Qscmf\\HttpTracer\\": "src/"
    }
  },
  "extra": {
    "qscmf": {
      "providers": [
        "Qscmf\\HttpTracer\\HttpTracerProvider"
      ]
    }
  }
}
